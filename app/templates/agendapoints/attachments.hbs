<AppChrome
  @editorDocument={{this.editorDocument}}
  @documentContainer={{this.documentContainer}}
>
  <:returnLink>
    <AgendapointBackLink @meetingId={{this.model.returnToMeeting}} />
  </:returnLink>
  <:actions>
    <AgendapointMenu @documentContainer={{this.documentContainer}} />
    <AuButton
      @disabled={{true}}
      @skin='naked'
      @icon='nav-up-down'
      @iconAlignment='right'
      class='au-u-padding-tiny'
    ><span class='au-u-h-functional au-u-regular'>{{t
          'utils.file-options'
        }}</span></AuButton>
    <AuButton @disabled={{true}}>{{t 'utils.save'}}</AuButton>
  </:actions>
</AppChrome>
<DocumentAttachments
  @documentContainer={{this.documentContainer}}
  @decisions={{this.decisions}}
/>