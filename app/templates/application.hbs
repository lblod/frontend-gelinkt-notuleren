{{page-title
  (concat
    'Gelinkt Notuleren'
    (if this.showEnvironment (concat ' - ' this.environmentName))
  )
}}
<AuModalContainer />
<AuApp class='{{if this.showEnvironment "au-c-app--environment"}}'>
  {{#if this.showEnvironment}}
    <div class='au-u-hide-on-print'>
      <EnvironmentBanner
        @environmentName={{this.environmentName}}
        @applicationName='Gelinkt Notuleren'
      />
    </div>
  {{/if}}
  {{outlet}}
</AuApp>
<AuToaster @position='top' class='toaster' />